{
    "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    },
    {
      "src": "backend/main.py",
      "use": "@vercel/python@4.3.0"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/main.py" },
    { "src": "/static/(.*)", "dest": "/frontend/build/static/$1" },
    { "src": "/(.*)", "dest": "/frontend/build/index.html" }
  ]
  }